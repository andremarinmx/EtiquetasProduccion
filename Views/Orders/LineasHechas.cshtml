@model IEnumerable<EtiquetasProduccion.Models.Line_Made>

@{
    ViewBag.Title = "LineasHechas";
}

<h2>Lineas Hechas</h2>

<table class="table">
    <tr>
        <th>
            @*@Html.DisplayNameFor(model => model.OrderNumber)*@
            Order Number
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Height)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Width)
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.Line_)*@
            Line
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.Pcs_Order)*@
            Order Pcs
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.Pcs_Box)*@
            Box Pcs
        </th>
        <th>
            Status
            @*@Html.DisplayNameFor(model => model.Status_)*@
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Folio)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.OrderNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Height)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Width)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Line_)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Pcs_Order)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Pcs_Box)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Status_)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Folio)
        </td>
        <td>
            @Html.ActionLink("Detalles", "DetallesLineasHechas", new { id=item.Id })
        </td>
    </tr>
}

</table>
